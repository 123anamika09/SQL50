
select  query_name,
round(
AVG(rating / position) ,2) as quality
,
-- use CASE inside aggregate functions like COUNT() or SUM() when you want to count or sum conditionally â€”      that is, only some rows based on a condition.
round((count(case when rating < 3 then 1 end) *100.0) / count(*) 
,2) as poor_query_percentage

from Queries 
GROUP BY query_name;
